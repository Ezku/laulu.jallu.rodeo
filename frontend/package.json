{
  "name": "frontend-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^20.0.6",
    "@types/node": "^8.0.20",
    "@types/parsimmon": "^1.3.0",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/storybook__react": "^3.0.3",
    "@types/webfontloader": "^1.6.28",
    "awesome-typescript-loader": "^3.2.2",
    "bloomer": "^0.6.2",
    "bulma": "^0.5.1",
    "font-awesome": "^4.7.0",
    "glamor": "^2.20.39",
    "glamorous": "^4.1.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "parsimmon": "^1.6.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts-ts": "^2.7.0",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-ts",
    "start-ts": "react-scripts-ts start",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar src/ -o src/ --include-path ./src --include-path ./node_modules",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --include-path ./src --include-path ./node_modules",
    "storybook": "npm-run-all -p watch-css watch-storybook",
    "watch-storybook": "start-storybook -p 6006"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.4"
  }
}
