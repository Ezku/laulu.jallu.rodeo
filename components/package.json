{
  "name": "@laulu.jallu.rodeo/components",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^21.1.1",
    "@types/node": "^8.0.31",
    "@types/parsimmon": "^1.3.0",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^15.5.5",
    "@types/storybook__react": "^3.0.5",
    "@types/webfontloader": "^1.6.29",
    "awesome-typescript-loader": "^3.2.3",
    "bloomer": "^0.6.2",
    "bulma": "^0.5.3",
    "font-awesome": "^4.7.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.9.7",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "parsimmon": "^1.6.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts-ts": "^2.7.0",
    "slugify": "^1.2.1",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-ts",
    "start-ts": "react-scripts-ts start",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css":
      "node-sass-chokidar src/ -o src/ --include-path ./src --include-path ./node_modules",
    "watch-css":
      "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --include-path ./src --include-path ./node_modules",
    "storybook": "npm-run-all -p watch-css watch-storybook",
    "watch-storybook": "start-storybook -p 6006",
    "dist": "npm-run-all -p dist-tsc",
    "dist-tsc": "tsc -p tsconfig.dist.json"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.11"
  }
}
